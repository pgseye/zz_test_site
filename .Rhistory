library(bayestestR)
library(ggplot2)
# Simulate some data
set.seed(253445)
dat <- simulate_simpson(
n = 50,
r = 0.5,
groups = 3,
difference = 2,
group_prefix = "G_"
)
dat$V1 <- dat$V1*(10)
#| label: plot_aggregated
#| message: false
ggplot(dat, aes(x = V1, y = V2)) +
geom_point() +
theme_bw(base_size = 20)
